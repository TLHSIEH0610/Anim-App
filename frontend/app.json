      "supportsTablet": true,
      "bundleIdentifier": "com.arnie.animapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": true,
      "package": "com.arnie.animapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3e5696ec-5c50-4902-b144-2e6d80a7bf53"
      }
    }
  }
}
